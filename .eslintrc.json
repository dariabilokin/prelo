{
  "extends": ["@salesforce/eslint-config-lwc/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "fetch": false
},  
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
